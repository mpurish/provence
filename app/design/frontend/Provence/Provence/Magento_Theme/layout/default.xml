<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">media/logo.jpg</argument>
                <argument name="logo_img_width" xsi:type="number">300</argument>
                <argument name="logo_img_height" xsi:type="number">300</argument>
            </arguments>
        </referenceBlock>
        <referenceContainer name="header.panel">
                <block class="Magento\Framework\View\Element\Template" name="header.phones" template="Magento_Theme::html/header/phones.phtml" />
                <block class="Magento\Framework\View\Element\Template" name="header.provence.links" template="Magento_Theme::html/header/provence-links.phtml" />
        </referenceContainer>
        <referenceBlock name="register-link" remove="true" />
        <!--<move element="header.provence.links" destination="header.panel" before="-"/>-->
    </body>
</page>